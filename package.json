    "build:pro": "vue-tsc && vite build --mode production",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "element-plus": "^2.9.7",
    "fast-glob": "^3.3.3",
    "husky": "^9.1.7",
    "mockjs": "^1.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-vue": "^1.1.6",
    "sass": "^1.86.1",
    "vite-plugin-mock": "^3.0.2",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/node": "^22.13.17",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "husky": "^8.0.0",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vue-tsc": "^2.2.4"
  }
}
